-- Scripts:

local function YOBCIM_fake_script() -- ForgottenMemories.Main 
	local script = Instance.new('LocalScript', ForgottenMemories)

	local ScreenGUI = script.Parent
	local MainFrame = ScreenGUI.MainFrame
	local Side = MainFrame.Side
	local ESPPLayer = MainFrame.ESPPlayerButton
	local ESPAnimatronics = MainFrame.ESPAnimatronicsButton
	local GotoArcade = MainFrame.GotoArcadeButton
	local GotoGenerator1 = MainFrame.GotoGenerator1Button
	local GotoGenerator2 = MainFrame.GotoGenerator2Button
	local GotoSecurityOffice = MainFrame.GotoSecurityOfficeButton
	local ExitTextLabel = MainFrame.ExitTextLabel
	local BringMarionetteInteract = MainFrame.BringMarionetteInteractButton
	local OriginalPosition = game.Workspace.WorkspaceUtilities.Curtains.InteractPart.CFrame
	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local RunService = game:GetService("RunService")
	local ESPPlayerBool = false
	local ESPAnimatronicsBool = false
	local MarionetteinteractBool = false
	local Enter= false
	UserInputService.InputBegan:Connect(function(input, chs)
		if not chs then
			if input.KeyCode == Enum.KeyCode.K then
				if Enter == false then
					Enter = true
					MainFrame.Visible = false
				else
					Enter = false
					MainFrame.Visible = true
				end
			end
		end
	end)
	BringMarionetteInteract.MouseButton1Click:Connect(function()
		if MarionetteinteractBool == false then
			MarionetteinteractBool = true
		else
			MarionetteinteractBool = false
		end
	end)
	MainFrame.MouseEnter:Connect(function()
		TweenService:Create(ExitTextLabel, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
			Size = UDim2.new(0.726, 0,0.101, 0);
			BackgroundTransparency = 0;
			TextTransparency = 0;
		}):Play()
	end)
	MainFrame.MouseLeave:Connect(function()
		TweenService:Create(ExitTextLabel, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
			Size = UDim2.new(0, 0,0.101, 0);
			BackgroundTransparency = 1;
			TextTransparency = 1;
		}):Play()
	end)
	ESPPLayer.MouseButton1Click:Connect(function()
		if ESPPlayerBool == false then
			ESPPlayerBool = true
		else
			ESPPlayerBool = false
		end
	end)
	ESPAnimatronics.MouseButton1Click:Connect(function()
		if ESPAnimatronicsBool == false then
			ESPAnimatronicsBool = true
		else
			ESPAnimatronicsBool = false
		end
	end)
	GotoArcade.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-274.623749, 4.02500057, -316.107574, 1, 0, 0, 0, 1, 0, 0, 0, 1)
	end)
	GotoGenerator1.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-395.288727, 4.05000019, -11.1899109, 0, 0, 1, 0, 1, 0, -1, 0, 0)
	end)
	GotoGenerator2.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-383.383972, 4.05000019, -48.2902222, -1, 0, 0, 0, 1, 0, 0, 0, -1)
	end)
	GotoSecurityOffice.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-79.1113052, 3.98608541, -150.104309, 0, 0, 0.999999583, 0, 1, 0, -1, 0, 0)
	end)
	game:GetService("RunService").RenderStepped:Connect(function()
		if MarionetteinteractBool == true then
			local interact = game.Workspace.WorkspaceUtilities.Curtains.InteractPart
			interact.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0.5, -2)
			TweenService:Create(interact, TweenInfo.new(0, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, 0, false, 0), {
				CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -1.5)
			}):Play()
		else
			local interact = game.Workspace.WorkspaceUtilities.Curtains.InteractPart
			interact.CFrame = OriginalPosition
		end
	end)
	while true do tick() wait(1)
		if ESPPlayerBool == true and ESPAnimatronicsBool == true then
			coroutine.resume(coroutine.create(function()
				for i,v in pairs(workspace:GetChildren()) do
					if v:IsA("Model") then
						if v:FindFirstChild("Humanoid") then
							for i2,v2 in pairs(game.Players:GetPlayers()) do
								if v2.Character.Name == v.Name then
									print("skip this model")
								else
	
									local Character = v
									local Humanoid = Character.Humanoid
									local HumanoidRootPart = Humanoid.RootPart
									local Head = Character.Head
	
									local BoxHandleAdornment = Instance.new("BoxHandleAdornment", Head)
									BoxHandleAdornment.Name = Head.Name.."_espBox"
									BoxHandleAdornment.Adornee = Head
									BoxHandleAdornment.Size = Head.Size
									BoxHandleAdornment.AlwaysOnTop = true
									BoxHandleAdornment.ZIndex = 0
									BoxHandleAdornment.Visible = true
	
									local BillboardGui = Instance.new("BillboardGui")
									local TextLabel = Instance.new("TextLabel")
									local UICorner = Instance.new("UICorner")
	
									BillboardGui.Parent = HumanoidRootPart
									BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
									BillboardGui.Active = true
									BillboardGui.AlwaysOnTop = true
									BillboardGui.LightInfluence = 1.000
									BillboardGui.Size = UDim2.new(2, 50, 2, 10)
	
									TextLabel.Parent = BillboardGui
									TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
									TextLabel.BackgroundTransparency = 0.500
									TextLabel.Size = UDim2.new(1, 0, 1, 0)
									TextLabel.Font = Enum.Font.Sarpanch
									TextLabel.Text = Character.Name
									TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
									TextLabel.TextScaled = true
									TextLabel.TextSize = 14.000
									TextLabel.TextWrapped = true
	
									UICorner.CornerRadius = UDim.new(500, 500)
									UICorner.Parent = TextLabel
	
									coroutine.resume(coroutine.create(function()
										wait(1)
										BillboardGui:Destroy()
										BoxHandleAdornment:Destroy()
									end))
	
								end
							end
						end
					end
				end
			end))
			return
		end
		if ESPPlayerBool == true then
			coroutine.resume(coroutine.create(function()
				for i,v in pairs(game.Players:GetPlayers()) do
					if v.Name == game.Players.LocalPlayer.Name then
					else
						local Character = v.Character
						local HumanoidRootPart = Character.HumanoidRootPart
						local Head = Character.Head
						local BoxHandleAdornment = Instance.new("BoxHandleAdornment", Head)
						BoxHandleAdornment.Name = Head.Name.."_espBoxPlayer"
						BoxHandleAdornment.Adornee = Head
						BoxHandleAdornment.Size = Head.Size
						BoxHandleAdornment.AlwaysOnTop = true
						BoxHandleAdornment.ZIndex = 0
						BoxHandleAdornment.Visible = true
						local BillboardGui = Instance.new("BillboardGui")
						local TextLabel = Instance.new("TextLabel")
						local UICorner = Instance.new("UICorner")
	
						BillboardGui.Parent = HumanoidRootPart
						BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
						BillboardGui.Active = true
						BillboardGui.AlwaysOnTop = true
						BillboardGui.LightInfluence = 1.000
						BillboardGui.Size = UDim2.new(2, 50, 2, 10)
	
						TextLabel.Parent = BillboardGui
						TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
						TextLabel.BackgroundTransparency = 0.500
						TextLabel.Size = UDim2.new(1, 0, 1, 0)
						TextLabel.Font = Enum.Font.Sarpanch
						TextLabel.Text = Character.Name
						TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
						TextLabel.TextScaled = true
						TextLabel.TextSize = 14.000
						TextLabel.TextWrapped = true
	
						UICorner.CornerRadius = UDim.new(500, 500)
						UICorner.Parent = TextLabel
						coroutine.resume(coroutine.create(function()
							wait(1)
							BillboardGui:Destroy()
							BoxHandleAdornment:Destroy()
						end))
					end
				end
			end))
		end;if ESPAnimatronicsBool == true then
			coroutine.resume(coroutine.create(function()
				for i,v in pairs(workspace:GetChildren()) do
					if v:IsA("Model") then
						if v:FindFirstChild("Humanoid") then
							local Character = v
							local Humanoid = Character.Humanoid
							local HumanoidRootPart = Humanoid.RootPart
							local Head = Character.Head
	
							local BoxHandleAdornment = Instance.new("BoxHandleAdornment", Head)
							BoxHandleAdornment.Name = Head.Name.."_espBox"
							BoxHandleAdornment.Adornee = Head
							BoxHandleAdornment.Size = Head.Size
							BoxHandleAdornment.AlwaysOnTop = true
							BoxHandleAdornment.ZIndex = 0
							BoxHandleAdornment.Visible = true
	
							local BillboardGui = Instance.new("BillboardGui")
							local TextLabel = Instance.new("TextLabel")
							local UICorner = Instance.new("UICorner")
	
							BillboardGui.Parent = HumanoidRootPart
							BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
							BillboardGui.Active = true
							BillboardGui.AlwaysOnTop = true
							BillboardGui.LightInfluence = 1.000
							BillboardGui.Size = UDim2.new(2, 50, 2, 10)
	
							TextLabel.Parent = BillboardGui
							TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
							TextLabel.BackgroundTransparency = 0.500
							TextLabel.Size = UDim2.new(1, 0, 1, 0)
							TextLabel.Font = Enum.Font.Sarpanch
							TextLabel.Text = Character.Name
							TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
							TextLabel.TextScaled = true
							TextLabel.TextSize = 14.000
							TextLabel.TextWrapped = true
	
							UICorner.CornerRadius = UDim.new(500, 500)
							UICorner.Parent = TextLabel
	
							coroutine.resume(coroutine.create(function()
								wait(1)
								BillboardGui:Destroy()
								BoxHandleAdornment:Destroy()
							end))
						end
					end
				end
			end))
			wait()
			for i,v in pairs(game.Players:GetPlayers()) do
				local Character = v.Character
				local HumanoidRootPart = Character.HumanoidRootPart
				local Head = Character.Head
				if Head:FindFirstChild(Head.Name.."_espBox") then
					Head:FindFirstChild(Head.Name.."_espBox"):Destroy()
				end
				if HumanoidRootPart:FindFirstChild("BillboardGui") then
					HumanoidRootPart:FindFirstChild("BillboardGui"):Destroy()
				end
			end
		end
	end
end
coroutine.wrap(YOBCIM_fake_script)()
